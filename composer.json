{
    "name": "musingsz/yii2-audit",
    "description": "No bootstrap dependent,Yii2 Audit records and requests, database changes, php/js errors and associated data.",
    "keywords": ["yii2", "audit", "trail", "logging"],
    "type": "yii2-extension",
    "license": "BSD 3-Clause",
    "require": {
        "php": ">=5.4.0",
        "ext-bcmath": "*",
        "yiisoft/yii2": "2.*",
        "yiisoft/yii2-debug": "2.*",
        "musingsz/yii2-chartjs-widget": "0.3",
        "phpspec/php-diff": "1.*"
    },
    "autoload": {
        "psr-4": {"musingsz\\yii2\\audit\\": "src"}
    },
    "extra": {
        "bootstrap": "musingsz\\yii2\\audit\\Bootstrap"
    }
}
