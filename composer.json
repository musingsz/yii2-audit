{
    "name": "musingsz/yii2-audit",
    "description": "Yii2 Audit records and displays web/cli requests, database changes, php/js errors and associated data.",
    "keywords": ["yii2", "audit", "trail", "logging"],
    "type": "yii2-extension",
    "license": "BSD 3-Clause",
    "require": {
        "php": ">=5.4.0",
        "ext-bcmath": "*",
        "phpspec/php-diff": "1.*"
    },
    "autoload": {
        "psr-4": {"musingsz\\yii2\\audit\\": "src"}
    },
    "extra": {
        "bootstrap": "musingsz\\yii2\\audit\\Bootstrap"
    }
}
